<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.mlight.domain">
 <class name="Message" table="v_message_all">
	<id name="id">
	   <column name="id"/>
	   <generator class="native"/>
	</id>
    <property name="organization_id">
        <column name="organization_id"/>
    </property>
     <property name="type">
        <column name="type"/>
    </property>
     <property name="content">
        <column name="content"/>
    </property>
     <property name="attachment">
        <column name="attachment"/>
    </property>
     <property name="create_time">
        <column name="create_time"/>
    </property>
     <property name="update_time">
        <column name="update_time"/>
    </property>
     <property name="delete_time">
        <column name="delete_time"/>
    </property>
     <property name="deleted">
        <column name="deleted"/>
    </property>
    <property name="sender">
        <column name="sender"/>
    </property>
    <property name="receiver">
        <column name="receiver"/>
    </property>
   <property name="sendername">
        <column name="sendername"/>
    </property>
    <property name="receivername">
        <column name="receivername"/>
    </property>
     <property name="groupxx">
        <column name="groupxx"/>
    </property>
        <property name="groupid">
        <column name="group_id"/>
    </property>
     
 </class>
</hibernate-mapping>
	