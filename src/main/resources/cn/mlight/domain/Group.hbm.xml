<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.mlight.domain">
 <class name="Group" table="t_group">
	<id name="id">
	   <column name="id"/>
	   <generator class="native"/>
	</id>
    <property name="organization_id">
        <column name="organization_id"/>
    </property>
     <property name="department_id">
        <column name="department_id"/>
    </property>
     <property name="name">
        <column name="name"/>
    </property>
     <property name="type">
        <column name="type"/>
    </property>
     <property name="creator">
        <column name="creator"/>
    </property>
     <property name="owner">
        <column name= "owner"/>
    </property>
     <property name="logo">
        <column name="logo"/>
    </property>
     <property name="logo_info">
        <column name="logo_info"/>
    </property>
     <property name="max_members">
        <column name="max_members"/>
    </property>
     <property name="create_time">
        <column name="create_time"/>
    </property>
     <property name="update_time">
        <column name="update_time"/>
    </property>
     <property name="delete_time">
        <column name="delete_time"/>
    </property>
    <property name="deleted">
        <column name="deleted"/>
    </property>
 </class>
</hibernate-mapping>
	