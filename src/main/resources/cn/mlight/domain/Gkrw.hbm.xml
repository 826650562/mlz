<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.mlight.domain">
	<class name="Gkrw" table="t_task">
		<id name="rwid">
			<column name="rwid" />
			<generator class="assigned" />
		</id>
		<property name="rwbh">
			<column name="rwbh" />
		</property>
		<property name="task_name">
			<column name="task_name" />
		</property>
		<property name="task_content">
			<column name="task_content" />
		</property>
		<property name="level">
			<column name="level" />
		</property>
		<property name="level_name">
			<column name="level_name" />
		</property>
		<property name="state">
			<column name="state" />
		</property>
		<property name="fzr_id">
			<column name="fzr_id" />
		</property>
		<property name="fzr_name">
			<column name="fzr_name" />
		</property>
		<property name="state_name">
			<column name="state_name" />
		</property>
		<property name="start_time">
			<column name="start_time" />
		</property>
		<property name="end_time">
			<column name="end_time" />
		</property>
	</class>
</hibernate-mapping>
	